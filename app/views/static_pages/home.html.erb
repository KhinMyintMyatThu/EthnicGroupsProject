</style>
<script>
  $(document).ready(function(){
    $(document).scroll(function(){
        var st = $(this).scrollTop();
        if(st > $('#services').offset().top) {
            $("nav").addClass('affix');
        } else {
            $("nav").removeClass('affix');
        }
    });
  });
</script>
